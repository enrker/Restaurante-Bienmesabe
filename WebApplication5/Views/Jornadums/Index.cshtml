@model IEnumerable<WebApplication5.Models.Jornadum>

@{
    ViewData["Title"] = "Index";
}

<h1>Jornadas</h1>

<p>
    <a asp-action="Create">Crear nuevo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Tipo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HoraEntrada)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HoraSalida)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Tipo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HoraEntrada)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HoraSalida)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdJornada">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdJornada">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdJornada">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
